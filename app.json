{
  "expo": {
    "name": "Flashcards :D",
    "slug": "MobileFlashcards",
    "privacy": "public",
    "sdkVersion": "35.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Sketch001.png",
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.mobileflashcards.exp"
    },
    "notification": {
      "icon": "./assets/brain-cenit-view.png"
    },
    "android": {
      "package": "org.mobileflashcards.exp"
    }
  }
}
